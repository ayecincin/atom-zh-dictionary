{
  "name": "atom-zh-dictionary",
  "main": "./lib/atom-zh-dictionary",
  "version": "0.0.0",
  "description": "A short description of your package",
  "keywords": [],
  "activationCommands": {
    "atom-workspace": "atom-zh-dictionary:mean"
  },
  "configSchema": {
    "defaultDictionary": {
      "type": "string",
      "default": "Shanbay",
      "enum": [
        "Shanbay",
        "OS X"
      ],
      "description": "Default dictionary to look up"
    },
    "closeWhenLoseFocus": {
      "default": true,
      "type": "boolean",
      "description": "auto close query result tab when it loses focus, not valid for OS X dictionary.app"
    }
  },
  "repository": "https://github.com/chendotjs/atom-zh-dictionary",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "request": "^2.79.0"
  }
}
